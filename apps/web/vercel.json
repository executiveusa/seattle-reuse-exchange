{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "includeFiles": [
          "next.config.js",
          ".env.local",
          ".env"
        ]
      }
    }
  ],
  "buildCommand": "cd ../.. && npx pnpm install --store=node_modules/.pnpm-store && cd apps/web && npx pnpm build",
  "installCommand": "cd ../.. && npx pnpm install --store=node_modules/.pnpm-store",
  "outputDirectory": ".next"
}